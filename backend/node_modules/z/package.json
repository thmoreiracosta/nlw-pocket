{
  "name": "z",
  "version": "1.0.1",
  "main": "src/z.js",
  "description": "pattern matching for javascript",
  "scripts": {
    "test": "NODE_PATH=. mocha tests/*.js",
    "coveralls": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "lint": "standard --fix \"src/z.js\" \"tests/*.js\"",
    "coverage": "nyc npm test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/z-pattern-matching/z"
  },
  "author": "Wagner Leonardi",
  "keywords": [
    "pattern",
    "matching",
    "functional",
    "haskell"
  ],
  "standard": {
    "globals": [
      "describe",
      "it",
      "after",
      "afterEach",
      "before",
      "beforeEach"
    ]
  },
  "nyc": {
    "statements": 100,
    "branches": 97.37,
    "functions": 100,
    "lines": 100,
    "check-coverage": true,
    "reporter": [
      "lcov",
      "text",
      "text-summary"
    ],
    "exclude": [
      "node_modules/**/*",
      "tests/*"
    ]
  },
  "bugs": {
    "url": "https://github.com/z-pattern-matching/z/issues"
  },
  "homepage": "https://github.com/z-pattern-matching/z#readme",
  "license": "Apache-2.0",
  "devDependencies": {
    "chai": "^3.4.0",
    "coveralls": "^2.11.16",
    "mocha": "^2.3.3",
    "mocha-lcov-reporter": "^1.2.0",
    "nyc": "^10.1.2",
    "standard": "^8.6.0"
  },
  "dependencies": {
    "deep-equal": "^1.0.1",
    "install": "^0.10.0",
    "js-function-reflector": "git+https://github.com/leonardiwagner/js-function-reflector.git"
  }
}
